<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="phoenix.dao.DualDao">

	<select id="query" parameterType="java.util.Map" resultType="string">
		SELECT 'x' FROM dual
	</select>

	<select id="queryMobile" parameterType="java.util.Map"
		resultType="string">
		SELECT data FROM mobile.originallog
		WHERE
		day = #{day}
		and get_json_object(substr(data,0,instr(data,"|")),"$.token") = #{token}
		and pt = #{pt}
		limit 100
	</select>

</mapper>
